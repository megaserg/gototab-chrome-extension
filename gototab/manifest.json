{
  "manifest_version": 2,

  "name": "Gototab",
  "description": "This extension lets you search for the tab",
  "version": "1.1",

  "background": {
    "page": "popup.html"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Cmd+Shift+K"
  },

  "permissions": [
    "tabs"
  ],

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      }
    }
  }
}
